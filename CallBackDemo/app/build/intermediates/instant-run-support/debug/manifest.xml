        android:supportsRtl="true"
        android:theme="@style/AppTheme" >
        <activity android:name="com.innofang.callbackdemo.MainActivity" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.innofang.callbackdemo.call_back_demo_2.LoadImageActivity" >
        </activity>
        <activity android:name="com.innofang.callbackdemo.call_back_demo_1.ClickRecyclerViewActivity" >
        </activity>

        <service
            android:name="com.android.tools.fd.runtime.InstantRunService"
            android:exported="true" />
    </application>

</manifest>